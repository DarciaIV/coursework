$creds=Get-Credential
$dcs="AKL1","CHC1","CHI2","COP2","DAL1","FRA2","HKG3","JOH2","LON4","LON5","LRK1","MIA2","MPOD","MUM1","PAR2","SAO2","SCS2","SIN4","SJL1","SYD3","TOK2","TOR1","WDC1" 

