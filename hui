
# Get current AD site
$DC = [System.DirectoryServices.ActiveDirectory.ActiveDirectorySite]::GetComputerSite().Name.ToUpper()
 
# Connect to vCenter
Connect-VIServer prod-vsv-$dc-1.vm.ironport.com
 
# Decrease memory reservations in the first proxy pool
Get-VM | where name -Like cws-prd-prox-* | Get-VMResourceConfiguration | Set-VMResourceConfiguration -MemReservationGB 24
